DATABASE_URL="postgresql://postgres:postgres@localhost:5432/myphotos"

# AWS S3 Configuration
AWS_S3_BUCKET="gff-myphotos"
AWS_REGION="ap-south-1"
AWS_ACCESS_KEY_ID=""
AWS_SECRET_ACCESS_KEY=""

# AWS SQS Configuration
AWS_SQS_QUEUE_URL="https://sqs.ap-south-1.amazonaws.com/813007935810/myphotos-gdrive-imports"
AWS_SQS_DLQ_URL="https://sqs.ap-south-1.amazonaws.com/813007935810/myphotos-gdrive-imports-dlq"
AWS_SQS_ACCESS_KEY_ID=""     # Can reuse AWS_ACCESS_KEY_ID if same IAM user
AWS_SQS_SECRET_ACCESS_KEY="" # Can reuse AWS_SECRET_ACCESS_KEY

# NextAuth Configuration
AUTH_SECRET=""                       # Generate with: openssl rand -base64 32
NEXTAUTH_URL="http://localhost:3000" # Change to your production URL in production
API_KEY="api_key"                    # Generate with: openssl rand -base64 32

# Upload Configuration
NEXT_PUBLIC_UPLOAD_BATCH_SIZE="5" # Number of images to upload and process in parallel

# Embedding Service Configuration
EMBEDDING_SERVICE_WORKERS="6" # Number of uvicorn worker processes for embedding service
EMBEDDING_SERVICE_URL="http://localhost:8000"

# Google Drive OAuth
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""
GOOGLE_REDIRECT_URI="http://localhost:3000/api/auth/google/callback"

# Import Worker Configuration
GDRIVE_IMPORT_BATCH_SIZE="10" # Files to process per worker cycle
GDRIVE_API_DELAY_MS="100"     # Delay between Google Drive API calls
GDRIVE_MAX_RETRIES="3"        # Max retries for failed files

# Encryption key for storing OAuth tokens
ENCRYPTION_KEY="" # Generate with: openssl rand -base64 32
